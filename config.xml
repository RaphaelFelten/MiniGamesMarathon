<?xml version='1.0' encoding='utf-8'?>
<widget id="io.rphlfltn.minigamesadventures" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Mini Games Adventures</name>
    <description>
        An adventure through mini games.
    </description>
    <author email="raphael.felten@gmail.com" href="http://cordova.io">
        Raphael Felten
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/assets/GoDownUnder/mrpoopybutthole.png" platform="android" width="57" height="57" density="mdpi" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
